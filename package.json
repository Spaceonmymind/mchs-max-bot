{
    "name": "mchs-max-bot",
    "version": "1.0.0",
    "type": "module",
    "main": "bot.js",
    "scripts": {
        "start:bot": "node bot.js",
        "start:admin": "node adminServer.js",
        "start": "concurrently \"npm run start:bot\" \"npm run start:admin\"",

        "dev:bot": "nodemon bot.js",
        "dev:admin": "nodemon adminServer.js",
        "dev": "concurrently \"npm run dev:bot\" \"npm run dev:admin\""
    },
    "dependencies": {
        "@maxhub/max-bot-api": "^0.2.1",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "concurrently": "^9.1.0"
    },
    "devDependencies": {
        "nodemon": "^3.1.4"
    }
}